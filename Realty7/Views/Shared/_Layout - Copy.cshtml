<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    
  <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <link href="~/Content/justified-nav.css" rel="stylesheet" />
</head>
<body>
      <div class="container" style="padding:0; background-color:white; max-width:941px">
    <nav role="navigation" class="navbar navbar-default">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" data-target="#navbarCollapse" data-toggle="collapse" class="navbar-toggle">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        <!-- Collection of nav links and other content for toggling -->
        <div id="navbarCollapse" class="collapse navbar-collapse">
            <ul class="nav nav-justified">
                <li id="home" class="active">
                    
                    <a href="/"><span style="display:block;padding-top:10px"></span>HOME</a>
                </li>
                <li id="about_us"><a href="/Service">ABOUT US</a></li>
                <li id="our_agents"><a href="/Technology">OUR AGENTS</a></li>
                <li id="search_property"><a href="/Clients">SEARCH PROPERTY</a></li>
                <li id="town_info"><a href="/Company">TOWN INFO</a></li>
                <li id="QA"><a href="/Company">Q/A</a></li>
                <li id="contacts"><a href="/Company">CONTACTS</a></li>
            </ul>
        </div>
    </nav>
    <div style="background-color:white">
              @RenderBody()
    </div>
</div>

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
